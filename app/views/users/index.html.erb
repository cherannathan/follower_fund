<div class="container">
  <div class="cards-index">
    <% @users.each do |user| %>
      <div class="card-product">
          <div class="card-product-infos mt-2">
            <h2><%= user.first_name %></h2>
            <p><%= user.last_name %></p>
            <p><%= user.pseudo %></p>
            <p><%= user.bio%></p>
            <br>
          <div class="avatar-bordered">
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>
